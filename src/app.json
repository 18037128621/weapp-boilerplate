{
  // All pages must be here.
  // First is default to view
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#pages
  "pages": [
    "pages/index/index",
    "pages/demo/demo"
    // dont remove this comment
  ],
  // Window config
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#window
  "window": {
    // 导航栏背景颜色，如"#000000"
    // HexColor
    "navigationBarBackgroundColor": "#000000",
    // 导航栏标题颜色，仅支持 black/white，默认：white
    // String
    "navigationBarTextStyle": "black",
    // 导航栏标题文字内容
    // String
    "navigationBarTitleText": "Index Page",
    // 窗口的背景色，默认：#ffffff
    // HexColor
    "backgroundColor": "#ffffff",
    // 下拉背景字体、loading 图的样式，仅支持 dark/light，默认：dark
    // String
    "backgroundTextStyle": "white",
    // 是否开启下拉刷新，详见页面相关事件处理函数，默认：false
    // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/app-service/page.html#页面相关事件处理函数
    // Boolean
    "enablePullDownRefresh": false
  },
  // Tab bar configuration
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#tabBar
  "tabBar": {
    "color": "#ccc",
    "selectedColor": "#35495e",
    "borderStyle": "white",
    "backgroundColor": "#f9f9f9",
    "list": [
      {
        "text": "HOME",
        "pagePath": "pages/index/index",
        "iconPath": "images/icon_component.png",
        "selectedIconPath": "images/icon_component_HL.png"
      },
      {
        "text": "DEMO",
        "pagePath": "pages/demo/demo",
        "iconPath": "images/icon_API.png",
        "selectedIconPath": "images/icon_API_HL.png"
      }
    ]
  },
  // Network timeout configuration
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#networkTimeout
  "networkTimeout": {
    "request": 10000,
    "downloadFile": 10000
  },
  // Debug log to console
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#debug
  "debug": true
}
